@using MVC1.ViewModels;
@model Login
<h1>Login</h1>
<form method="post" asp-action="SubmitLogin" asp-controller="AuthUser">

    <div>
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div>
        <label asp-for="Passwored"></label>
        <input asp-for=Passwored class="form-control" type="password" />
    </div>

    <input type="submit" class="btn btn-primary mt-5" value="Login" />
</form>